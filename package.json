{
  "name": "jira-got",
  "version": "0.2.0",
  "description": "Convenience wrapper for `got` to interact with the Jira API",
  "main": "index.js",
  "scripts": {
    "lint": "eslint source.js",
    "test": "babel-node node_modules/.bin/babel-istanbul cover _mocha && codecov",
    "compile": "babel --presets es2015 --plugins add-module-exports source.js -o index.js",
    "prepublish": "npm run lint && npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/angusfretwell/jira-got.git"
  },
  "author": "Angus Fretwell <angus@inoutput.io> (http://angusfretwell.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/angusfretwell/jira-got/issues"
  },
  "homepage": "https://github.com/angusfretwell/jira-got#readme",
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-eslint": "^6.0.2",
    "babel-istanbul": "^0.8.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-airbnb": "^1.1.1",
    "babel-preset-es2015": "^6.6.0",
    "codecov": "^1.0.1",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-import": "^1.4.0",
    "get-stream": "^2.0.0",
    "mocha": "^3.1.0",
    "should": "^8.3.1"
  },
  "dependencies": {
    "got": "^6.3.0"
  }
}
